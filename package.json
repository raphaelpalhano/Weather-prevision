{
  "name": "good-time-surf",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/main.js",
    "start:dev": "nodemon",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "npm test -- -c jest-e2e.config.ts --runInBand",
    "test:unit": "npm test -- -c jest-unit.config.ts --coverage",
    "test:unit:watch": "npm run test:watch -- -c jest-unit.config.ts --coverage",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "testcontainers": "^9.8.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
